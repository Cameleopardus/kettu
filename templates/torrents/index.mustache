{{#torrents}}
  <li id="{{id}}" class="torrent">
    <strong>{{name}}</strong><br />
    <small>{{progressDetails}}</small><br />
    
    <div class="progress">
      <div class="ui-progressbar ui-widget ui-widget-content ui-corner-all">
        {{{progressBar}}} 
      </div>
      {{{pauseAndActivateButton}}}      
    </div>
    
    <div class="status">{{statusString}}</div>
  </li>
{{/torrents}}